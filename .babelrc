{
  "presets": ["next/babel"],
  "plugins": [
    ["styled-components", { "ssr": true }],
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "@": "./src",
        "@public": "./public",
        "@root": "./"
      }
    }],
    [
      "import", {
        "libraryName": "antd"
      }
    ]
  ]
}
